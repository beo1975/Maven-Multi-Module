<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<context-param>
		<param-name>ContextConfigLocation</param-name>
		<param-value>classpath:/beans.xml</param-value>
	</context-param>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>CreateUserServlet</servlet-name>
		<display-name>CreateUserServlet</display-name>
		<description></description>
		<servlet-class>com.benito.user.servlets.CreateUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DisplayUserDetailsServlet</servlet-name>
		<display-name>DisplayUserDetailsServlet</display-name>
		<description></description>
		<servlet-class>com.benito.user.servlets.DisplayUserDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateUserServlet</servlet-name>
		<url-pattern>/CreateUserServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DisplayUserDetailsServlet</servlet-name>
		<url-pattern>/DisplayUserDetailsServlet</url-pattern>
	</servlet-mapping>
</web-app>
